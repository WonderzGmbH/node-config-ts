{
  "name": "@wonderzgmbh/node-config-ts",
  "version": "1.0.0-alpha.1",
  "files": [
    "dist/**/*.js",
    "bin/**/*.ts",
    "package.json"
  ],
  "main": "./dist/index.js",
  "bin": "./bin/cli",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "audit:prod": "yarn audit --groups dependencies",
    "lint": "eslint --max-warnings=0 --ext ts,tsx src",
    "test": "mocha",
    "test:cli": "yarn compile && NODE_ENV=production NODE_CONFIG_TS_DIR=./dist/src/__tests__/__fixtures__/config OUTPUT_FILENAME=config.d.ts OUTPUT_FOLDER=./ node ./bin/cli",
    "compile": "tsc",
    "pretest": "yarn compile",
    "prepublish": "yarn compile"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "json-to-ts": "^1.6.0",
    "minimist": "^1.2.7",
    "prettier": "^2.7.1",
    "ramda": "^0.27.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@types/ramda": "^0.26.40",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.1.0",
    "semantic-release": "^17.0.2",
    "travis-deploy-once": "^5.0.9",
    "typescript": "^4.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": "git://github.com/WonderzGmbH/node-config-ts.git",
  "license": "MIT"
}
